{
  "version": 2,
  "buildCommand": "./node_modules/.bin/vite build && npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
  "installCommand": "npm install",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[...route]"
    }
  ]
}
